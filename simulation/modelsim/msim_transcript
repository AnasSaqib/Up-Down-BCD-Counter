force -freeze sim:/counter/pb_1 1 0
force -freeze sim:/counter/pb_2 1 0
force -freeze sim:/counter/clk 1 0, 0 {50 ps} -r 100
run -all
add wave -position end  sim:/counter/pb_1
add wave -position end  sim:/counter/pb_2
add wave -position end  sim:/counter/clk
